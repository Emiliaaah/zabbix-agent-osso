            </applications>
            <items>
                <item>
                    <name>SMART: Has SMART disk</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>smart[has_disk]</key>
                    <delay>1h</delay>
                    <history>30d</history>
                    <trends>90d</trends>
                    <applications>
                        <application>
                            <name>SMART</name>
                        </application>
                    </applications>
                </item>
