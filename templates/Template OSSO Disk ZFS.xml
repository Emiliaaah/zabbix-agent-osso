                <item>
                    <name>ZFS ARC cache hit ratio</name>
                    <type>CALCULATED</type>
                    <key>zfs.arcstats_hit_ratio</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <trends>90d</trends>
                    <units>%o</units>
                    <params>1000*((last(zfs.arcstats[hits])+0.001)/(last(zfs.arcstats[hits])+last(zfs.arcstats[misses])+0.001))</params>
                    <applications>
