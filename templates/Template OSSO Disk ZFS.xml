                    <delay>5m</delay>
                    <history>30d</history>
                    <trends>90d</trends>
                    <units>B</units>
                    <params>last(zfs.arcstats[hits])+last(zfs.arcstats[misses])</params>
                    <applications>
                        <application>
                            <name>ZFS</name>
                        </application>
                    </applications>
